{
	"redis_host":	"${REDIS_HOST}",
	"redis_port":	"${REDIS_PORT}",
	"redis_auth":	"${REDIS_PASS}",

	"http_host":	"0.0.0.0",
	"http_port":	7379,
	"threads":	4,

	"daemonize":	true,

